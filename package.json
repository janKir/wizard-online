{
  "name": "wizard-online",
  "version": "1.0.0-alpha.1",
  "description": "Online platform for playing the Wizard card game",
  "main": "index.tsx",
  "scripts": {
    "start": "parcel index.html -d dist/app",
    "build:app": "parcel build index.html -d dist/app",
    "build:server": "parcel build src/server.ts --target node -d dist/server",
    "heroku-postbuild": "yarn run build:server",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "yarn run lint:es && yarn run lint:style",
    "lint:es": "eslint --ignore-path .gitignore \"src/**/*.{ts,tsx,js}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx,js}\"",
    "server:watch": "parcel src/server.ts --target node -d dist/server",
    "server:serve": "nodemon dist/server/server.js -w dist/server",
    "dev-server": "concurrently --kill-others -n \"parcel,nodemon\" -p \"{time}-{name}\" -t \"HH:mm:ss\" \"yarn run server:watch\" \"yarn run server:serve\""
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Jan Kirchner, Fredo :-)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "@material-ui/core": "^4.9.7",
    "@sentry/browser": "^5.15.4",
    "@sentry/node": "^5.15.4",
    "boardgame.io": "^0.39.2",
    "core-js": "^3.6.4",
    "date-fns": "^2.12.0",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.5",
    "styled-components": "^5.0.1"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-unicorn": "^18.0.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.3",
    "prettier": "^2.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.4.3"
  }
}
